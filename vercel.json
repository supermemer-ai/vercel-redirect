{
    "redirects": [
        {
            "source": "/",
            "destination": "https://www.supermeme.ai",
            "permanent": true
        },
        {
              "source": "/blog",
              "destination": "https://supermeme.superblog.cloud/"  
            },
            {
              "source": "/blog/(.*?)([.]\\w{2,5})(\\?[^/]+)?",
              "destination": "https://supermeme.superblog.cloud/$1$2$3"
            },
            {
              "source": "/blog/([\\S\\s]+)",
              "destination": "https://supermeme.superblog.cloud/$1/"
            },
            {
              "source": "/_elderjs/svelte/(.*?)([.]\\w{2,5})(\\?[^/]+)?",
              "destination": "https://supermeme.superblog.cloud/_elderjs/svelte/$1$2$3"
            },
            {
              "source": "/_elderjs/svelte/([\\S\\s]+)",
              "destination": "https://supermeme.superblog.cloud/_elderjs/svelte/$1/"
            },
            {
              "source": "/.netlify/functions/(.*?)([.]\\w{2,5})(\\?[^/]+)?",
              "destination": "https://supermeme.superblog.cloud/.netlify/functions/$1$2$3"
            },
            {
              "source": "/.netlify/functions/([\\S\\s]+)",
              "destination": "https://supermeme.superblog.cloud/.netlify/functions/$1/"
            },
        {
            "source": "/:path*",
            "destination": "https://www.supermeme.ai/:path*",
            "permanent": true
        }
    ]
}
